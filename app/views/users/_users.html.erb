<% users.each do |user|%>
  <div class='general_info_alt'>
    <div id='points'>
      <p>
      <%= user.points %> 
      </p>

    </div>
    <h1>
      <%= link_to user do %>
        <%= image_tag user.image, width: 25 %>
        <%= user.name.titleize %>
      <% end %>
    </h1>
    <div class='clear'></div>
    <p class='email'><%= mail_to user.email %></p>
  </div>
<%end%>
